<?xml version="1.0" encoding="UTF-8"?><beans xmlns="http://www.springframework.org/schema/beans"       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"       xmlns:context="http://www.springframework.org/schema/context"       xsi:schemaLocation="http://www.springframework.org/schema/beans    http://www.springframework.org/schema/beans/spring-beans.xsd    http://www.springframework.org/schema/context    http://www.springframework.org/schema/context/spring-context.xsd">    <bean id="preProcessorToLower" class="edu.school21.spring.preprocessor.PreProcessorToLowerImpl"/>    <bean id="preProcessorToUpper" class="edu.school21.spring.preprocessor.PreProcessorToUpperImpl"/><!--    <bean id="rendererErrToLower" class="edu.school21.spring.renderer.RendererErrImpl">--><!--        <constructor-arg type="edu.school21.spring.preprocessor.PreProcessorToLowerImpl" ref="preProcessorToLower"/>--><!--    </bean>-->    <bean id="rendererErrToLower" class="edu.school21.spring.renderer.RendererErrImpl">        <constructor-arg ref="preProcessorToLower"/>    </bean>    <bean id="rendererErrToUpper" class="edu.school21.spring.renderer.RendererErrImpl">        <constructor-arg ref="preProcessorToUpper"/>    </bean>    <bean id="rendererStandardToLower" class="edu.school21.spring.renderer.RendererStandardImpl">        <constructor-arg ref="preProcessorToLower"/>    </bean>    <bean id="rendererStandardToUpper" class="edu.school21.spring.renderer.RendererStandardImpl">        <constructor-arg ref="preProcessorToUpper"/>    </bean>    <bean id="printerWithPrefixErrToLower" class="edu.school21.spring.printer.PrinterWithPrefixImpl">        <constructor-arg ref="rendererErrToLower"/>    </bean>    <bean id="printerWithPrefixErrToUpper" class="edu.school21.spring.printer.PrinterWithPrefixImpl">        <constructor-arg ref="rendererErrToUpper"/>    </bean>    <bean id="printerWithPrefixStandardToLower" class="edu.school21.spring.printer.PrinterWithPrefixImpl">        <constructor-arg ref="rendererStandardToLower"/>    </bean>    <bean id="printerWithPrefixStandardToUpper" class="edu.school21.spring.printer.PrinterWithPrefixImpl">        <constructor-arg ref="rendererStandardToUpper"/>    </bean>    <bean id="printerWithDateTimeErrToLower" class="edu.school21.spring.printer.PrinterWithDateTimeImpl">        <constructor-arg ref="rendererErrToLower"/>    </bean>    <bean id="printerWithDateTimeErrToUpper" class="edu.school21.spring.printer.PrinterWithDateTimeImpl">        <constructor-arg ref="rendererErrToUpper"/>    </bean>    <bean id="printerWithDateTimeStandardToLower" class="edu.school21.spring.printer.PrinterWithDateTimeImpl">        <constructor-arg ref="rendererStandardToLower"/>    </bean>    <bean id="printerWithDateTimeStandardToUpper" class="edu.school21.spring.printer.PrinterWithDateTimeImpl">        <constructor-arg ref="rendererStandardToUpper"/>    </bean></beans>